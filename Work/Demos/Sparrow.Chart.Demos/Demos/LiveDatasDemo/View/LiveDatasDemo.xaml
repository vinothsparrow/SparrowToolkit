<UserControl x:Class="Sparrow.Chart.Demos.Demos.LiveDatasDemo.LiveDatasDemo"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:chart="http://sparrowtoolkit.codeplex.com/wpf"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <chart:SparrowChart Margin="5" RenderingMode="Default" Theme="Metro"  x:Name="Chart" >
            <chart:SparrowChart.XAxis>
                <chart:LinearXAxis Interval="20" ShowCrossLines="True" Header="Point" StringFormat="0.0"/>
            </chart:SparrowChart.XAxis>
            <chart:SparrowChart.YAxis>
                <chart:LinearYAxis ShowCrossLines="True" StringFormat="0.0" Header="Value" >
                </chart:LinearYAxis>
            </chart:SparrowChart.YAxis>
            <chart:LineSeries UseSinglePart="True">
            </chart:LineSeries>
        </chart:SparrowChart>
    </Grid>
</UserControl>
